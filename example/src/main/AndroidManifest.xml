<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="jp.co.thcomp.example">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application android:allowBackup="true" android:label="@string/app_name"
                 android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme"
                 android:name="android.support.multidex.MultiDexApplication">

        <activity
            android:name="jp.co.thcomp.testapi.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="jp.co.thcomp.testapi.TestLockPatternViewActivity" android:label="@string/app_name" />
        <activity android:name="jp.co.thcomp.testapi.TestCalendarViewActivity" android:label="@string/app_name" />
    </application>

</manifest>
